{
  "extends": ["prettier/react", "eslint-config-airbnb"],
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "import/prefer-default-export": 0,
    "linebreak-style": 0,
    "semi": "off",
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0
  }
}
